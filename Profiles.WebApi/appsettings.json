{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5050"
      }
    }
  },

  "AllowedHosts": "*"
}
